{
  "name": "react-motion-drawer-fixed",
  "version": "1.0.0",
  "scripts": {
    "build": "babel lib --out-dir ./dist ",
    "build-example": "webpack --config example/webpack.config.js",
    "serve": "node example/server.js",
    "open": "opn http://localhost:8888/example/index.html",
    "lint": "eslint lib",
