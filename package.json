    "major-release":
      "npm run build && npm version major && npm publish && git push --follow-tags"
  },
  "main": "./dist/drawer.js",
  "repository": {
    "type": "git",
    "url": "https://github.com/hackerhgl/react-motion-drawer-fixed.git"
  },
  "keywords": [
