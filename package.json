    "navigation"
  ],
  "bugs": {
    "url": "https://github.com/hackerhgl/react-motion-drawer-fixed/issues"
  },
  "homepage": "https://github.com/hackerhgl/react-motion-drawer-fixed",
  "devDependencies": {
    "babel-cli": "^6.23.0",
    "babel-core": "^6.23.1",
    "babel-eslint": "^7.1.1",
    "babel-loader": "^6.3.2",
    "babel-preset-env": "^1.1.10",
    "babel-preset-latest": "^6.24.1",
    "babel-preset-react": "^6.23.0",
    "eslint": "^3.6.3",
    "eslint-plugin-react": "^3.6.3",
    "file-loader": "^0.10.1",
    "opn-cli": "^1.0.0",
    "react": "^15.4.2",
    "react-dom": "^15.4.2",
